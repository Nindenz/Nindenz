----------------------------------------Game-
local Tab = Window:NewTab("Game")
local Game = Tab:NewSection("PrisonLife")

Game:NewButton("prisonlife 1", "PLG GUI V3.2", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/RbxCheats/RbxCheats/master/Scripts/PLG%20GUIs/PLG%20v3.2.lua", true))()
