----------------------------------------Setting-
local Tab = Window:NewTab("Setting")
local Setting = Tab:NewSection("Keybind")

Setting:NewKeybind("Keybing ui", "Yes Read", Enum.KeyCode.P, function()
	Library:ToggleUI()
end)

local Setting = Tab:NewSection("Setting")

Setting:NewTextBox("WalkSpeed", "Speed", function(txt)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (txt)
end)

Setting:NewTextBox("JumpPower", "Jump", function(txt)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = (txt)
end)

local Setting = Tab:NewSection("Executor")

Setting:NewButton("Synapse X", "Executor?", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Nindenz/Nindenz/main/Synapse%20x", true))()
end)

