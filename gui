----------------------------------------LocalPlayer-
local Tab = Window:NewTab("LocalPlayer")
local LocalPlayer = Tab:NewSection("Local")

LocalPlayer:NewSlider("Speed", "Speed", 1000, 0, function(s) -- 1000 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

LocalPlayer:NewSlider("JumpPo", "Jump", 1000, 0, function(s) -- 1000 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

LocalPlayer:NewButton("Reset s/j", "reset speed jump", function()
 game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
 game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end)

LocalPlayer:NewButton("Respawn", "reset character", function()
game.Players.LocalPlayer.Character.Humanoid:Destroy()
end)

LocalPlayer:NewButton("GodMode", "INF health with respawn", function()
loadstring(game:HttpGet("https://pastebin.com/raw/jyVVfCGk",true))()
end)

LocalPlayer:NewTextBox("TpToPlayer", "TextboxInfo", function(txt)

end)

