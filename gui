local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Roblox Hack (Version-1.1)", "Ocean")

----------------------------------------Script-
local Tab = Window:NewTab("Script")
local Script = Tab:NewSection("Fun")

Script:NewButton("Punch", "Punch player to fling", function()
loadstring(game:HttpGet('https://pastebin.com/raw/YPZ0wNaw'))()
end)

Script:NewButton("UFling", "Near player to fling", function()
loadstring(game:HttpGet("https://pastebin.com/raw/GfwkiUTV", true))()
end)

Script:NewButton("Rejoin", "Rejoin game you play", function()
loadstring(game:HttpGet("https://pastebin.com/raw/yVdXMaFy",true))()
end)

