----------------------------------------Setting-
local Tab = Window:NewTab("Setting")
local Setting = Tab:NewSection("Keybind")

Setting:NewKeybind("Keybing ui", "Yes Read", Enum.KeyCode.P, function()
	Library:ToggleUI()
end)

